source /root/variables

radio0=$(cat /etc/config/wireless | grep -m 1 "option key" | cut -d "'" -f 2)
radio1=$(cat /etc/config/wireless | grep -m 2 "option key" | cut -d "'" -f 2 | tail -n1)
date=$(/root/plugins/get_date)

echo "${model}"
echo "${version}"
echo "${date}"
printf "\n"
echo '_'"WiFi Password"'_'
echo "*2.4GHz Band:* ${radio0}"
if [ ! -z "$radio1" ]; then
echo "*5GHz Band:* ${radio1}"
fi
